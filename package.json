        "@sveltejs/kit": "^2.15.1",
        "@sveltejs/vite-plugin-svelte": "^4.0.0",
        "@types/mousetrap": "^1.6.15",
        "ansi_up": "^6.0.2",
        "autoprefixer": "^10.4.20",
        "bits-ui": "^1.3.13",
        "clsx": "^2.1.1",
        "driver.js": "^1.3.1",
        "embla-carousel-svelte": "^8.5.2",
        "formiojs": "^4.21.6",
        "formsnap": "^2.0.0-next.1",
        "lucide-svelte": "^0.462.0",
        "mode-watcher": "^0.5.0",
