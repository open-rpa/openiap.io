{
    "name": "opencore-web",
    "version": "0.0.7",
    "type": "module",
    "main": "build/index.js",
    "scripts": {
        "run": "vite build && vite preview",
        "dev": "vite dev",
        "dev:debug": "vite dev -d",
        "build:dev": "vite build -m dev",
